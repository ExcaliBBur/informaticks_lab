<?xml version="1.0" ?>
<all>
	<debug type="str">on</debug>
	<window type="dict">
		<title type="str">Sample Konfabulator Widget</title>
		<name type="str">main_window</name>
		<width type="int">500</width>
		<height type="int">500</height>
	</window>
	<image type="dict">
		<src type="str">Images/Sun.png</src>
		<name type="str">sun1</name>
		<hOffset type="int">250</hOffset>
		<vOffset type="int">250</vOffset>
		<alignment type="str">center</alignment>
	</image>
	<text type="dict">
		<data type="str">Click Here</data>
		<size type="int">36</size>
		<style type="str">bold</style>
		<name type="str">text1</name>
		<hOffset type="int">250</hOffset>
		<vOffset type="int">100</vOffset>
		<alignment type="str">center</alignment>
		<onMouseUp type="str">sun1.opacity = (sun1.opacity / 100) * 90;</onMouseUp>
	</text>
</all>
